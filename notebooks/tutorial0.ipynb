        "# Import all packages\n", "from meshcat.animation import Animation\n",
        "import numpy as np\n", "import pydynorrt as pyrrt\n",
        "from pydynorrt import pin_more as pyrrt_vis # Small utility functions "
        "to visualize motion planning\n",
        "import pinocchio as pin  # Not needed in the current script (consider "
        "removing if not used)\n",
        "import meshcat\n", "import time\n", "import matplotlib.pyplot as plt"
      ]
    },
    {
      "cell_type" : "code",
